<%- include('./../fragment/header') %>

    <section>
      <p>
        <a href="/board" class="btn">목록보기</a>
        <a href="/board/pwd/edit/<%=id%>" class="btn edit">수정</a>
        <a href="/board/pwd/del/<%=id%>" class="btn del">삭제</a>
      </p>
      <table class="smit_elem">
        <caption>글 내용</caption>
        <colgroup>
          <col style="width: 150px" />
          <col style="width: 300px" />
          <col style="width: 150px" />
          <col />
        </colgroup>
        <thead></thead>
        <tfoot></tfoot>
        <tbody>
          <tr>
            <th scope="row">작성자</th>
            <td><%=name%></td>
            <th scope="row">등록일</th>
            <td><%=date%></td>
          </tr>
          <tr>
            <th scope="row">제목</th>
            <td colspan="3"><%=title%></td>
          </tr>
          <tr>
            <th scope="row">글내용</th>
            <td colspan="3"><%=content%></td>
          </tr>
        </tbody>
      </table>
    </section>

<%- include('./../fragment/footer') %>